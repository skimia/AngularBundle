{{'{{ module_var }}'}}.factory('{{name}}', ['$repo',
	function($repo){
		return $repo.instance({
			$type     : '{{name}}',
			$settings : {
				$url     : 'api/{{plurialise(name)|lower}}/:{{name|lower}}Id.json',
				$params  : {
					{{name|lower}}Id: "@id"
				},
				$load    : {
					for : 'all'
				} 
			}
		});
	}]);